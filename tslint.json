{
    "extends": [
      "tslint:recommended", 
      "tslint-react", 
      "tslint-plugin-prettier", 
      "tslint-config-prettier"  
    ],
    "rules": {
      "semicolon": true,
      "prettier": true,
      "quotemark": [
        true,
        "single",
        "avoid-escape",
        "avoid-template",
        "jsx-double"
      ]
    }
}